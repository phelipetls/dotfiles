{
  "coc.preferences.floatActions": false,
  "coc.preferences.promptInput": false,
  "diagnostic.enableMessage": "jump",
  "diagnostic.checkCurrentLine": true,
  "signature.hideOnTextChange": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "▪  ",
  "diagnostic.enableSign": false,
  "diagnostic.errorSign": "▎",
  "diagnostic.warningSign": "▎",
  "diagnostic.hintSign": "▎",
  "diagnostic.infoSign": "▎",
  "[javascript][typescript][javascripreact][typescriptreact]": {
    "coc.preferences.formatOnSave": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.bracketEnterImprove": false,
  "signature.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "suggest.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "diagnostic.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "hover.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "css.lint.unknownAtRules": "ignore",
  "html.filetypes": ["html", "htmlhugo", "htmldjango"],
  "prettier.requireConfig": true,
  "prettier.formatterPriority": 101,
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.onIgnoredFiles": "warn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "astro",
    "mdx"
  ],
  "suggest.noselect": true,
  "html.autoCreateQuotes": false,
  "html.autoClosingTags": false,
  "coc.source.syntax.enable": true,
  "coc.source.syntax.filetypes": ["htmlhugo"],
  "colors.enable": true,
  "highlight.colorNames.enable": false,
  "inlayHint.enable": false,
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "filetypes": [
        "sh",
        "zsh",
        "yaml",
        "lua",
        "vim",
      ],
      "formatterPriority": 100
    }
  },
  "Lua.hint.enable": false,
  "sumneko-lua.enableNvimLuaDev": true
}
