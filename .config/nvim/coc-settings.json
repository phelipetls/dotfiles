{
  "coc.preferences.floatActions": false,
  "coc.preferences.promptInput": false,
  "diagnostic.enableMessage": "jump",
  "diagnostic.checkCurrentLine": true,
  "signature.hideOnTextChange": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "▪  ",
  "diagnostic.enableSign": false,
  "diagnostic.errorSign": "▎",
  "diagnostic.warningSign": "▎",
  "diagnostic.hintSign": "▎",
  "diagnostic.infoSign": "▎",
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "javascriptreact", "typescriptreact"],
  "typescript.updateImportsOnFileMove.enable": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.bracketEnterImprove": false,
  "signature.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "suggest.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "diagnostic.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "hover.floatConfig": {"border": true, "borderhighlight": "VertSplit"},
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "filetypes": ["sh", "zsh", "yaml", "lua", "python", "javascript", "typescript", "javascriptreact", "typescriptreact", "vim"],
      "formatterPriority": 100
    },
    "tailwindcss-lsp": {
      "command": "tailwindcss-language-server",
      "args": ["--stdio"],
      "filetypes": [
        "css",
        "html",
        "htmlhugo",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "rootPatterns": [
        "tailwind.config.js",
        "tailwind.config.ts",
        "postcss.config.js",
        "postcss.config.ts"
      ],
      "initializationOptions": {
        "userLanguages": {
          "htmlhugo": "html"
        }
      },
      "settings": {
        "tailwindCSS": {
          "validate": true,
          "lint": {
            "cssConflict": "warning",
            "invalidApply": "error",
            "invalidScreen": "error",
            "invalidVariant": "error",
            "invalidConfigPath": "error",
            "invalidTailwindDirective": "error",
            "recommendedVariantOrder": "warning"
          },
          "classAttributes": ["class", "className", "classList", "ngClass"],
          "experimental": {}
        }
      }
    }
  }
}
